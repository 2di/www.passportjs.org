doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/app.css')
    != LRScript
  body
    .main-hold
      .top-site
        .search
          form(action='#')
            input(placeholder='Search for Strategies', type='text')
            button(type='submit')

        .social
          ul
            li.twitter
              a(href='#')
            li.facebook
              a(href='#')
            li.git
              a(href='#')
                span.stat 0

      block content

    .cloned-blur
    .search-con
      .head
        span.close-ico
        .hold
          h2 SEARCH FOR STRATEGIES
          form(action='#')
            .input
            span.autocomplete
            input(value='', type='text')
          p.info-line 110 STRATEGIES
      .con-line
      .results
        section

    script(src='//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
    script(src='javascripts/plugins.js')
    script(src='javascripts/main.js')